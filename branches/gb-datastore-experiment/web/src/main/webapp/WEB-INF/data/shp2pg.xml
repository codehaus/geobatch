<?xml version="1.0" encoding="UTF-8"?>
<FlowConfiguration>
	<workingDirectory>SHP2PG</workingDirectory>
    <autorun>true</autorun>
	<EventConsumerConfiguration>
		<workingDirectory>SHP2PG</workingDirectory>
		<performBackup>true</performBackup>
		<FileEventRule>
			<optional>false</optional>
			<originalOccurrencies>1</originalOccurrencies>
			<regex>.*\.shp</regex>
			<id>r1</id>
			<description>description</description>
			<name>test</name>
		</FileEventRule>
		<FileEventRule>
			<optional>false</optional>
			<originalOccurrencies>1</originalOccurrencies>
			<regex>.*\.shx</regex>
			<id>r2</id>
			<description>description</description>
			<name>test</name>
		</FileEventRule>
		<FileEventRule>
			<optional>false</optional>
			<originalOccurrencies>1</originalOccurrencies>
			<regex>.*\.dbf</regex>
			<id>r3</id>
			<description>description</description>
			<name>test</name>
		</FileEventRule>
		<FileEventRule>
			<optional>true</optional>
			<originalOccurrencies>1</originalOccurrencies>
			<regex>.*\.prj</regex>
			<id>r4</id>
			<description>description</description>
			<name>test</name>
		</FileEventRule>
		<Shp2PgActionConfiguration>
			<workingDirectory>SHP2PG</workingDirectory>
			<dbPWD>postgres</dbPWD>
			<dbUID>postgres</dbUID>
			<dbServerIp>localhost</dbServerIp>
			<dbType>postgis</dbType>
			<dbPort>5432</dbPort>			
			<dbName>test</dbName>
			<dbTableName>dcf</dbTableName>
			<serviceID>shp2PgfileGeneratorService</serviceID>
			<id>n1</id>
			<description>description</description>
			<name>test</name>
		</Shp2PgActionConfiguration>
		<id>id</id>
		<description>description</description>
		<name>test</name>
	</EventConsumerConfiguration>
	<eventGeneratorConfiguration class="FsEventGeneratorConfiguration">
		<wildCard>*.*</wildCard>
		<watchDirectory>SHP2PG/in</watchDirectory>
        <keepFiles>true</keepFiles>
		<osType>OS_UNDEFINED</osType>
		<eventType>FILE_ADDED</eventType>
		<id>id</id>
        <serviceID>fsEventGeneratorService</serviceID>
		<description>description</description>
		<name>test</name>
	</eventGeneratorConfiguration>
	<id>SHP2PG</id>
	<description>SHP2PG: testing injection of sparse shapefiles to postgis</description>
	<name>SHP2PG</name>
</FlowConfiguration>
