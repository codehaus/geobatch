<?xml version="1.0" encoding="UTF-8"?>
<FlowConfiguration>
	<workingDirectory>flow2workingdir</workingDirectory>
    <!--ftpserverPort -->
	<EventConsumerConfiguration>
		<workingDirectory>flow2workingdir</workingDirectory>
		<performBackup>false</performBackup>

       <FileEventRule>
			<optional>false</optional>
			<originalOccurrencies>1</originalOccurrencies>
			<regex>.*\.shp</regex>
			<id>r1</id>
			<description>description</description>
			<name>test</name>
		</FileEventRule>

        <FtpServerActionConfiguration>
			<workingDirectory>flow2workingdir</workingDirectory>
			<dataTransferMethod/>
			<ftpserverUSR>io</ftpserverUSR>
			<ftpserverPWD>xxx</ftpserverPWD>
			<ftpserverHost>127.0.0.1</ftpserverHost>
            <ftpserverPort/>
			<serviceID>shapefileFtpServerEventAction</serviceID>
			<id>a1</id>
			<description>description</description>
			<name>test</name>
		</FtpServerActionConfiguration>

		<id>id</id>
		<description>description</description>
		<name>test</name>
	</EventConsumerConfiguration>
	<eventGeneratorConfiguration class="FtpEventGeneratorConfiguration">
		<wildCard>*.*</wildCard>
		<watchDirectory>flow2workingdir/in</watchDirectory>
		<osType>OS_UNDEFINED</osType>
		<eventType>FILE_ADDED</eventType>
        <serviceID>ftpEventGeneratorService</serviceID>
		<id>id</id>
        <ftpserverUSR>ftpuser</ftpserverUSR>
		<ftpserverPWD>test</ftpserverPWD>
        <ftpserverPort>8021</ftpserverPort>
		<description>description</description>
		<name>test</name>
	</eventGeneratorConfiguration>

    <id>flowFtpFMPush</id>
	<description>test globlal ftp flow: upload and push</description>
	<name>flowFtpFMPush</name>
</FlowConfiguration>