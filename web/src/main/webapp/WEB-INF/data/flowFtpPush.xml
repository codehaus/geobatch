<?xml version="1.0" encoding="UTF-8"?>
<FlowConfiguration>
	<workingDirectory>flow2workingdir</workingDirectory>
	<EventConsumerConfiguration>
		<workingDirectory>flow2workingdir</workingDirectory>
		<performBackup>false</performBackup>

       <FileEventRule>
			<optional>false</optional>
			<originalOccurrencies>1</originalOccurrencies>
			<regex>.*\.shp</regex>
			<id>r1</id>
			<description>description</description>
			<name>test</name>
		</FileEventRule>

        <FtpServerActionConfiguration>
			<workingDirectory>flow2workingdir</workingDirectory>
			<dataTransferMethod/>
			<ftpserverPWD>xxxxx</ftpserverPWD>
			<ftpserverUSR>ftpuser</ftpserverUSR>
			<ftpserverHost>ftp.server.com</ftpserverHost>
            <!-- TODO ftpserverPort -->
			<serviceID>shapefileFtpServerEventAction</serviceID>
			<id>a1</id>
			<description>description</description>
			<name>test</name>
		</FtpServerActionConfiguration>

		<id>id</id>
		<description>description</description>
		<name>test</name>
	</EventConsumerConfiguration>

	<eventGeneratorConfiguration class="FsEventGeneratorConfiguration">
		<wildCard>*.*</wildCard>
         <watchDirectory>flow1workingdir/in</watchDirectory>
		<osType>OS_UNDEFINED</osType>
		<eventType>FILE_ADDED</eventType>
        <serviceID>fsEventGeneratorService</serviceID>
        <!-- TODO ftpserverPort -->
		<id>id</id>
		<description>description</description>
		<name>test</name>
	</eventGeneratorConfiguration>

    <id>flowFtpPush</id>
	<description>test ftp push</description>
	<name>flowFtpPush</name>
</FlowConfiguration>