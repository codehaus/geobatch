			<optional>false</optional>
			<originalOccurrencies>1</originalOccurrencies>
			<regex>.*\.*</regex>
			<id>r1</id>
			<description>description</description>
			<name>test</name>
		</FileEventRule>
		
		<RegistryActionConfiguration>
			<workingDirectory>hdf4flowworkingdir</workingDirectory>
			<crs>EPSG:4326</crs>
			<envelope/>
			<dataTransferMethod>DIRECT</dataTransferMethod>
			<geoserverPWD>geoserver</geoserverPWD>
			<geoserverUID>admin</geoserverUID>
			<geoserverURL>http://localhost:8081/geoserver</geoserverURL>
			<metocDictionaryPath>d:/docs/documentation/NURC-2009/Super-Ensemble/metoc-dictionary.xml</metocDictionaryPath>
			<metocHarvesterXMLTemplatePath>d:/docs/documentation/NURC-2009/Super-Ensemble/Nurc-Cim_Schema/2009_11_25/example/iso-remsens-template.xml</metocHarvesterXMLTemplatePath>
			<!-- storeFilePrefix>c41078a</storeFilePrefix -->
			<wmsPath>/</wmsPath>
			<defaultNamespace>nurc</defaultNamespace>
			<defaultStyle>raster</defaultStyle>
			<styles/>
			<!-- styles>
				<style>raster.sld</style>
			</styles -->
			<serviceID>HDF42GeoTIFFsGeneratorService</serviceID>
			<id>a2</id>
			<description>description2</description>
			<name>test2</name>
		</RegistryActionConfiguration>
		
		<id>id</id>
		<description>description</description>
		<name>test</name>
	</EventConsumerConfiguration>
	
	<eventGeneratorConfiguration class="FsEventGeneratorConfiguration">
		<wildCard>*.*</wildCard>
		<watchDirectory>hdf4flowworkingdir/in</watchDirectory>
		<osType>OS_UNDEFINED</osType>
		<eventType>FILE_ADDED</eventType>
		<id>HDF4FlowId</id>
        <serviceID>fsEventGeneratorService</serviceID>
		<description>description</description>
		<name>test</name>
	</eventGeneratorConfiguration>
	
	<id>HDF4Flow</id>
	<description>HDF4Flow</description>
	<name>HDF4Flow</name>
</FlowConfiguration>