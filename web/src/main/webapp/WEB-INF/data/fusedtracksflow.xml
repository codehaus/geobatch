<?xml version="1.0" encoding="UTF-8"?>
<FlowConfiguration>
	<workingDirectory>FusedTracksContacts</workingDirectory>
    <autorun>true</autorun>
	<EventConsumerConfiguration>
		<workingDirectory>FusedTracksContacts</workingDirectory>
		<performBackup>true</performBackup>
		<FileEventRule>
			<optional>false</optional>
			<originalOccurrencies>1</originalOccurrencies>
			<regex>.*\.txt</regex>
			<id>fd</id>
			<description>description</description>
			<name>test</name>
		</FileEventRule>
		<FusedTrackActionConfiguration>
			<workingDirectory>FusedTracksContacts</workingDirectory>
			<logType>fused</logType>
			<stepTimeSecond>3600</stepTimeSecond>
			<cogDegreeThreshold>5.0</cogDegreeThreshold>
			<serviceID>fusedTrackGeneratorService</serviceID>
			<id>ft1</id>
			<description>description</description>
			<name>test</name>
		</FusedTrackActionConfiguration>
		<id>ftId</id>
		<description>description</description>
		<name>test</name>
	</EventConsumerConfiguration>
	<eventGeneratorConfiguration class="FsEventGeneratorConfiguration">
		<wildCard>*.*</wildCard>
		<watchDirectory>FusedTracksContacts/in</watchDirectory>
        <keepFiles>true</keepFiles>
		<osType>OS_UNDEFINED</osType>
		<eventType>FILE_ADDED</eventType>
		<id>tfId</id>
        <serviceID>fsEventGeneratorService</serviceID>
		<description>description</description>
		<name>test</name>
	</eventGeneratorConfiguration>
	<id>flowFusedTracks</id>
	<description>flowFusedTracks: testing injection of flowFusedTracks through Hibernate Spatial</description>
	<name>flowFusedTracks</name>
</FlowConfiguration>