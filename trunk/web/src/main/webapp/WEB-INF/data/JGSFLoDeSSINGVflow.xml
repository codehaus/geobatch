<?xml version="1.0" encoding="UTF-8"?>
<FlowConfiguration>

	<workingDirectory>JGSFLoDeSSworkingdir/INGV</workingDirectory>

	<EventConsumerConfiguration>
		<workingDirectory>JGSFLoDeSSworkingdir/INGV</workingDirectory>
		<performBackup>false</performBackup>
		<FileEventRule>
			<optional>false</optional>
			<originalOccurrencies>1</originalOccurrencies>
			<regex>.*\.*</regex>
			<id>r1</id>
			<description>description</description>
			<name>test</name>
		</FileEventRule>

		<MetocActionConfiguration>
			<workingDirectory>JGSFLoDeSSworkingdir/INGV</workingDirectory>
			<crs>EPSG:4326</crs>
			<envelope/>
			<!-- storeFilePrefix>c41078a</storeFilePrefix -->
			<metocDictionaryPath>G:/docs/documentation/NURC-2009/Super-Ensemble/metoc-dictionary.xml</metocDictionaryPath>
			<metocHarvesterXMLTemplatePath>G:/docs/documentation/NURC-2009/Super-Ensemble/Nurc-Cim_Schema/2009_11_25/example/iso-models-template.xml</metocHarvesterXMLTemplatePath>
			<serviceID>JGSFLoDeSSIGNVGeneratorService</serviceID>
			<id>a1</id>
			<description>description1</description>
			<name>test</name>
		</MetocActionConfiguration>
		
		<RegistryActionConfiguration>
			<workingDirectory>JGSFLoDeSSworkingdir/INGV</workingDirectory>
			<crs>EPSG:4326</crs>
			<envelope/>
			<dataTransferMethod>DIRECT</dataTransferMethod>
			<geoserverPWD>geoserver</geoserverPWD>
			<geoserverUID>admin</geoserverUID>
			<geoserverURL>http://localhost:8080/geoserver</geoserverURL>
			<registryURL>http://localhost:8080/nurccatalogue/webservice</registryURL>
			<providerURL>http://localhost:8081/geobatch/rest/publish</providerURL>
			<metocDictionaryPath>G:/docs/documentation/NURC-2009/Super-Ensemble/metoc-dictionary.xml</metocDictionaryPath>
			<metocHarvesterXMLTemplatePath>G:/docs/documentation/NURC-2009/Super-Ensemble/Nurc-Cim_Schema/2009_11_25/example/iso-models-template.xml</metocHarvesterXMLTemplatePath>
			<!-- storeFilePrefix>c41078a</storeFilePrefix -->
			<wmsPath>/</wmsPath>
			<defaultNamespace>nurc</defaultNamespace>
			<defaultStyle>raster</defaultStyle>
			<styles/>
			<!-- styles>
				<style>raster.sld</style>
			</styles -->
			<serviceID>NetCDFCFGeodetic2GeoTIFFsGeneratorService</serviceID>
			<id>a2</id>
			<description>description2</description>
			<name>test2</name>
		</RegistryActionConfiguration>
		
		<ImageMosaicActionConfiguration>
			<workingDirectory>JGSFLoDeSSworkingdir/INGV</workingDirectory>
			<crs>EPSG:4326</crs>
			<envelope/>
			<dataTransferMethod>EXTERNAL</dataTransferMethod>
			<geoserverPWD>geoserver</geoserverPWD>
			<geoserverUID>admin</geoserverUID>
			<geoserverURL>http://localhost:8080/geoserver</geoserverURL>
			<wmsPath>/</wmsPath>
			<defaultNamespace>nurc</defaultNamespace>
			<defaultStyle>raster</defaultStyle>
			<styles/>
			<timeRegex>[0-9]{8}T[0-9]{7}Z(?!.*[0-9]{8}T[0-9]{7}Z.*)</timeRegex>
			<elevationRegex>\\d{4}</elevationRegex>
			<serviceID>ImageMosaicGeneratorService</serviceID>
			<id>ImageMosaicService</id>
			<description>ImageMosaicService</description>
			<name>ImageMosaicService</name>
		</ImageMosaicActionConfiguration>
		
		<id>id</id>
		<description>description</description>
		<name>test</name>
	</EventConsumerConfiguration>
	
	<eventGeneratorConfiguration class="FsEventGeneratorConfiguration">
		<wildCard>*.*</wildCard>
		<watchDirectory>JGSFLoDeSSworkingdir/INGV/in</watchDirectory>
		<osType>OS_UNDEFINED</osType>
		<eventType>FILE_ADDED</eventType>
		<id>JGSFLoDeSSINGVId</id>
        <serviceID>fsEventGeneratorService</serviceID>
		<description>description</description>
		<name>test</name>
	</eventGeneratorConfiguration>
	
	<id>JGSFLoDeSSINGVflow</id>
	<description>JGSFLoDeSSINGVflow</description>
	<name>JGSFLoDeSSINGVflow</name>
</FlowConfiguration>